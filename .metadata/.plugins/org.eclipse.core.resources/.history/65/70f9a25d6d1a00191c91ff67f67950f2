package com.shad.domain.battlefield;

public enum BattlefieldPosition {
	TOPLEFT(0),
	TOPRIGHT(1),
	BOTTOM_LEFT(0),
	BOTTOM_RIGHT(1),
	PLAYER_TEAM_1(2),
	PLAYER_TEAM_2(3),
	PLAYER_TEAM_3(4),
	;
	
	private int index;
	private BattlefieldPosition(int index) {
		this.index = index;
	}
	public int getIndex() {
		return this.index;
	}
}
